    <AssemblyTitle>Data Experts Group - Connections Library - Restful Web Services</AssemblyTitle>
    <VersionPrefix>0.2.4</VersionPrefix>
    <Authors>Gary Holland</Authors>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>dexih.connections.webservice.restful</AssemblyName>
